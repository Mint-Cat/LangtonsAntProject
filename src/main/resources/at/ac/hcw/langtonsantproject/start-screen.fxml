<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="at.ac.hcw.langtonsantproject.Controller.StartScreenController"
            alignment="CENTER"
            hgap="10"
            vgap="10"
            fx:id="gridPane"
            >
            <Label fx:id="startscreen"/>
<VBox fx:id="parent" >
        <Label fx:id="NameLabel" alignment="CENTER"> Langton's Ant </Label>
        <Button fx:id="NewAntButton" text="New Ant" onAction="#NewAntButtonClick" alignment="CENTER"/>
        <Button fx:id="LoadAntButton" text="Load Ant" onAction="#LoadAntButtonClick" alignment="CENTER"/>
        <Button fx:id="DeleteAntButton" text="Delete Ant" onAction="#DeleteAntButtonClick" alignment="CENTER"/>
</VBox>
</GridPane>
